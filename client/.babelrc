{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "babel-plugin-transform-object-rest-spread",
    "syntax-async-functions",  
    [
      "transform-class-properties",
      {
        "loose": true
      }
    ]
  ]
}